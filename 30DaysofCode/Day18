    Stack<char> st = new Stack<char>();
    Queue<char> qu = new Queue<char>();

    public void pushCharacter(char ch)
    {
        st.Push(ch);
    }

    public void enqueueCharacter(char ch)
    {
        qu.Enqueue(ch);
    }

    public char popCharacter()
    {
        char x = ' ';
        x = st.Pop();
        return x;
    }

    public char dequeueCharacter()
    {
        char x = ' ';
        x = qu.Dequeue();
        return x;
    }
